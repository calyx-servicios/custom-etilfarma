<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <record model="ir.sequence" id="sale_order_external">
        <field name="name">External Sale Order</field>
        <field name="implementation">no_gap</field>
        <field name="prefix">SOE</field>
        <field name="padding">5</field>
        <field name="number_increment">1</field>
        <field name="sale_seq">True</field>
    </record>

    <record model="ir.sequence" id="sale_order_local">
        <field name="name">Local sale Order</field>
        <field name="implementation">no_gap</field>
        <field name="prefix">SOL</field>
        <field name="padding">5</field>
        <field name="number_increment">1</field>
        <field name="sale_seq">True</field>
    </record>

    <record model="ir.sequence" id="sale.seq_sale_order">
        <field name="sale_seq">True</field>
    </record>
</odoo>