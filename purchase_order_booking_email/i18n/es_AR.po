"                <p>Best regards,</p>\n"
"                <p style=\"color:#888888;\">\n"
"                % if object.sudo().create_uid and object.sudo().create_uid.signature:\n"
"                    ${object.sudo().create_uid.signature | safe}\n"
"                % endif\n"
"                </p>\n"
"                \n"
"        "
